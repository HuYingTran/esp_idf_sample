{
    "write_flash_args" : [ "--flash_mode", "dio",
                           "--flash_size", "detect",
                           "--flash_freq", "40m" ],
    "flash_settings" : {
        "flash_mode": "dio",
        "flash_size": "detect",
        "flash_freq": "40m"
    },
    "flash_files" : {
        "0x1000" : "bootloader/bootloader.bin",
        "0x20000" : "main.bin",
        "0xc000" : "partition_table/partition-table.bin",
        "0x11000" : "ota_data_initial.bin",
        "0x321000" : "explorer.bin"
    },
    "bootloader" : { "offset" : "0x1000", "file" : "bootloader/bootloader.bin", "encrypted" : "true" },
    "app" : { "offset" : "0x20000", "file" : "main.bin", "encrypted" : "true" },
    "partition-table" : { "offset" : "0xc000", "file" : "partition_table/partition-table.bin", "encrypted" : "true" },
    "otadata" : { "offset" : "0x11000", "file" : "ota_data_initial.bin", "encrypted" : "true" },
    "explorer" : { "offset" : "0x321000", "file" : "explorer.bin", "encrypted" : "false" },
    "extra_esptool_args" : {
        "after"  : "no_reset",
        "before" : "default_reset",
        "stub"   : true,
        "chip"   : "esp32"
    }
}
